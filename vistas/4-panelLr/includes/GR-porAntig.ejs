<!-- Stock Total -->
<div>
	<h2 class="">Stock Total Inicial</h2>
	<table>
		<thead>
			<tr>
				<th>Inicial</th>
				<th>Actual</th>
				<th>Δ</th>
				<th>%</th>
				<th>Proy.</th>
				<th>Δ</th>
				<th>%</th>
			</tr>
		</thead>
		<tbody>
			<tr id="total">
				<td><%= comp.moneda(valorLrInicial) %></td>
				<!-- Actual -->
				<td><%= comp.moneda(valorLrActual) %></td>
				<td><%= comp.moneda(valorLrActual - valorLrInicial) %></td>
				<td><%= compLrActualLrInicial %>%</td>
				<!-- Proyectada -->
				<td><%= comp.moneda(valorLrProy) %></td>
				<td><%= comp.moneda(valorLrInicial - valorLrProy) %></td>
				<td ><%= compLrProyLrInicial %>%</td>
			</tr>
		</tbody>
	</table>
</div>
