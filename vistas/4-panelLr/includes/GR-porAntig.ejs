<!-- Stock 123 -->
<div>
	<h2 class="">Stock de LR Inicial</h2>
	<table>
		<thead>
			<tr>
				<th>Inicial</th>
				<th>Actual</th>
				<th>Δ</th>
				<th>%</th>
				<th>Proy.</th>
				<th>Δ</th>
				<th>%</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td><%= comp.moneda(valorLr123Inicial) %></td>
				<!-- Actual -->
				<td><%= comp.moneda(valorLr123Actual) %></td>
				<td>-<%= comp.moneda(valorLr123Actual - valorLr123Inicial) %></td>
				<td><%= compLr123ActualLr123Inicial %>%</td>
				<!-- Proyectada -->
				<td><%= comp.moneda(valorLr123Proy) %></td>
				<td>+<%= comp.moneda(valorLr123Inicial - valorLr123Proy) %></td>
				<td><%= compLr123ProyLr123Inicial %>%</td>
			</tr>
		</tbody>
	</table>
</div>

